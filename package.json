{
	"name": "RetroScript",
	"displayName": "RetroScript",
	"description": "RetroScript syntax highlighting",
	"version": "0.5.0",
	"publisher": "xeeynamo & rubberduckycooly",
	"repository": "https://github.com/rubberduckycooly/vscode-rsdk",
	"license": "Apache-2.0",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Programming Languages", "Snippets"
	],
	"keywords": [
		"RSDK",
		"Retro Engine",
		"Retro SDK",
		"Retro Software Development Kit",
		"script",
		"sonic",
		"disassembly",
		"Christian Whitehead"
	],
	"contributes": {
		"languages": [
			{
				"id": "retroscript",
				"aliases": [
					"RSDK",
					"rsdk",
					"RetroScript"
				],
				"extensions": [
					".txt",
					".srsdk"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "retroscript",
				"scopeName": "source.retroscript",
				"path": "./syntaxes/retroscript.tmLanguage.json"
			}
		],

		"snippets": [
			{
				"language": "rsdk",
				"path": "./rsdk-snippets.json"
			}
		]
	}
}
